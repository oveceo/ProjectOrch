{
  "name": "eo-orchestrator",
  "version": "1.0.0",
  "description": "EO Project Orchestrator - Full-stack application for project management with Smartsheet integration",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3005",
    "build": "prisma generate && prisma db push --skip-generate && next build",
    "start": "next start --port ${PORT:-3005}",
    "lint": "next lint",
    "test": "jest",
    "test:e2e": "playwright test",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "type-check": "tsc --noEmit",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.0",
    "@azure/identity": "^4.11.1",
    "@azure/msal-browser": "^3.17.0",
    "@azure/msal-node": "^2.9.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.48.0",
    "@opentelemetry/exporter-jaeger": "^1.25.1",
    "@opentelemetry/sdk-node": "^0.51.1",
    "@prisma/client": "^5.14.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.394.0",
    "next": "14.2.4",
    "next-auth": "^4.24.7",
    "nodemailer": "^6.9.13",
    "pino": "^8.21.0",
    "prisma": "^5.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "smartsheet": "^3.1.4",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "jest": "^29.7.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.7.3",
    "typescript": "^5.4.5"
  }
}
